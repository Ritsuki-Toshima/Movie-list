<%= render "layouts/navbar"%>
<div class="container">
  <h1 class="mt-3 ">Your List</h1>
  <div class="lists">
    <% @lists.each do |list| %>
      <%= link_to list_path(list) do %>
        <figure class="snip1176">
          <img src="<%= list.photo.attached? ? cl_image_path(list.photo.key) : 'https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTPQDYau3Hs4-xw1i8jVSUY4BlF4FLmg8lQqg&usqp=CAU' %>" alt="list image"/>
          <figcaption>
            <div class="icon"><span><i class="ion-ios-star-outline"></i></span></div>
            <h2><%= list.name %></h2>
            <div class="caption">
              <p>See movies</p>
            </div>
          </figcaption>
        </figure>
      <% end %>
    <% end %>
  </div>
</div>
